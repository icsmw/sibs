test($a: a | b) [
    IF $a == "a" [
        "true";
    ] ELSE [
        "false";
    ];
];
test($a: a | b; $b: a | b) [
    IF $a == "a" && $b == "b" [
        "true";
    ] ELSE [
        "false";
    ];
];
test($a: {string}) [
    IF $a == "a" [
        "true";
    ] ELSE [
        "false";
    ];
];
test($a: {string}; $b: a | b) [
    IF $a == "a" && $b == "b" [
        "true";
    ] ELSE [
        "false";
    ];
];
test($a: {string}; $b: a | b; $c: {string}) [
    IF $a == "a" && $b == "b" && $c == "c" [
        "true";
    ] ELSE [
        "false";
    ];
];