#(a)
    @test($a: a | b) {
        $b = 1234;
        $c = true;
        if $a == "a" && $b == true && $c == 1234 {
            "true";
        } else {
            "false";
        };
    };
#(b)
    @test($a: {string}, $b: a | b, $c: {string}) {
        if $a == 1 && $b == true && $c == false {
            "true";
        } else {
            "false";
        };
    };
#(c)
    @test($a: {string}, $b: a | b, $c: {string}) {
        :b:test(1, true, false);
    };
#(d)
    @test($a: {string}, $b: a | b, $c: {string}) {
        :a:test(true);
    };