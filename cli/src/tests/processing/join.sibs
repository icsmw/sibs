/// Testing join 
#(join: ../../../../target/debug)
    @test_a {
        join (
            `./exit 0 100 1000 10`,
            `./exit 0 200 1000 10`,
            `./exit 0 300 1000 10`,
            `./exit 0 400 1000 10`,
        );
    };
    @test_b {
        join (
            `./exit 0 100 1000 10`,
            :self:test_a,
        );
    };
    @test_fail_a {
        `./exit 1 300 1000 10`;
    };
    @test_fail_b {
        `./exit 1 400 1000 10`;
    };
    @test_fail_all {
        join (
            `./exit 1 100 1000 10`,
            `./exit 1 200 1000 10`,
            `./exit 1 300 1000 10`,
            `./exit 1 400 1000 10`,
        );
    };
    // One with error
    @test_c {
        $status = join (
            `./exit 0 100 1000 10`,
            `./exit 0 200 1000 10`,
            `./exit 1 300 1000 10`,
            `./exit 0 400 1000 10`,
        );
        $status.is_any_fail();
    };
    // All with errors
    @test_d {
        $status = join (
            `./exit 1 100 1000 10`,
            `./exit 1 200 1000 10`,
            `./exit 1 300 1000 10`,
            `./exit 1 400 1000 10`,
        );
        $status.is_any_fail();
    };
    @test_e {
        $status = join (
            `./exit 0 100 1000 10`,
            :self:test_fail_a,
        );
        $status.is_any_fail();
    };
    @test_f {
        $status = join (
            `./exit 0 100 1000 10`,
            :self:test_fail_a,
            :self:test_fail_b,
        );
        $status.is_any_fail();
    };
    @test_g {
        $status = :self:test_fail_all;
        $status.is_any_fail();
    };
    @test_j {
        :self:test_fail_all.is_any_fail();
    };
