test [
    $a = @env::os;
    @env::is_os($a);
];
test [
    $a = @env::os;
    $b = @env::is_os($a) >> @str::repeat(3);
    IF $b == "truetruetrue" [
        "true";
    ];
];
test [
    IF @test::return_bool(true) [
        "true";
    ];
];
test [
    IF !@test::return_bool(false) [
        "true";
    ];
];