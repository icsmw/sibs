@test($a: a | b) {
    if $a == "a" {
        "true";
    } else {
        "false";
    };
};
@test($a: a | b; $b: a | b) {
    if $a == "a" && $b == "b" {
        "true";
    } else {
        "false";
    };
};
@test($a: {string}) {
    if $a == "a" {
        "true";
    } else {
        "false";
    };
};
@test($a: {string}; $b: a | b) {
    if $a == "a" && $b == "b" {
        "true";
    } else {
        "false";
    };
};
@test($a: {string}; $b: a | b; $c: {string}) {
    if $a == "a" && $b == "b" && $c == "c" {
        "true";
    } else {
        "false";
    };
};