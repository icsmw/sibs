/// Testing join 
#(join: ../../../../target/debug)
    emitter [
        JOIN (
            `./exit 0 100 1000 10`;
            `./exit 0 200 1000 10`;
            `./exit 0 300 1000 10`;
            `./exit 0 1000 1000 10`;
        );
        @sig::emit("a");
        "true";
    ];
    waiter_a [
        @sig::wait("a");
        @cx::set_var("a";"ok a");
        "true";
    ];
    waiter_b [
        @sig::wait("a");
        @cx::set_var("b";"ok b");
        "true";
    ];
    run [
        JOIN (
            :self:emitter;
            :self:waiter_a;
            :self:waiter_b;
        );
    ]