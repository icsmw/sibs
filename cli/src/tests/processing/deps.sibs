#(a)
    test($a: a | b) [
        IF $a == "a" [
            "one";
        ] ELSE [
            "---";
        ];
    ];
#(b)
    test($a: a | b; $b: {string}) [
        IF $a == "a" && $b == "b" [
            "two";
        ] ELSE [
            "---";
        ];
    ];
#(c_run)
    test($a: a | b; $b: {string})(
        $res_a = :a:test($a);
        $res_b = :b:test($a; $b);
    ) [
        @logs::info("output from :a:test($a) => {$res_a}");
        @logs::info("output from :b:test($b) => {$res_b}");
        IF $res_a == "one" && $res_b == "two" [
            "true";
        ] ELSE [
            "false";
        ];
    ];
#(d)
    test($a: a | b) [
        $var_from_comp_d = "changed_{$a}";
    ];
#(e)
    test($a: a | b; $b: {string}) [
        $var_from_comp_e = "changed_{$a}_{$b}";
    ];
#(f_run)
    test($a: a | b; $b: {string})(
        :d:test($a);
        :e:test($a; $b);
    ) [
        @logs::info("$var_from_comp_d => {$var_from_comp_d}");
        @logs::info("$var_from_comp_e => {$var_from_comp_e}");
        IF $var_from_comp_d == "changed_a" && $var_from_comp_e == "changed_a_b" [
            "true";
        ] ELSE [
            "false";
        ];
    ];
