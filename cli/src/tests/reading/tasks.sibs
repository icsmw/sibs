/// Building electron application
/// {$mode:dev} build developing version
/// {$mode:prod} build production version
@build($mode: dev | prod) {
    env::is_os(window) => $file_name = "release{$version}-win.zip";
    env::is_os(linux) => $file_name = "release{$version}-linux.zip";
    env::is_os(darwin) => $file_name = "release{$version}-darwin.zip";
    env::is_os(linux) => `tgz ./release/unpacked-linux {$file_name}`;
    env::is_os(window) => "release{$version}-win.zip";
    env::is_os(window) => {
        cd(something);
        `zip ./release/unpacked-win {$file_name}`;
        cd(..);
    };
    if $variable == "one" || $variable == "two" {
        cd(./linux);
    } else {
        cd(./linux);
    };
    if $variable == "one" {
        cd(./linux);
    };
    if $variable == "one" || $variable == "two" {
        cd(./linux);
    };
};
